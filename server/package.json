{
  "name": "omihorizn-server",
  "version": "1.0.0",
  "description": "Node.js + Express backend API for OmiHorizn - Global Mobility Platform",
  "main": "index.js",
  "author": "Omimek Technology Limited",
  "license": "PROPRIETARY",
  "engines": {
    "node": ">=22.14.0"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "build": "echo 'No build step needed'",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "docker:build": "docker build -t omihorizn-server .",
    "docker:run": "docker run -p 5000:5000 --env-file .env omihorizn-server"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.930.0",
    "@aws-sdk/s3-request-presigner": "^3.920.0",
    "@genkit-ai/compat-oai": "^1.23.0",
    "@genkit-ai/google-genai": "^1.21.0",
    "@supercharge/request-ip": "^1.2.0",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "bull": "^4.13.1",
    "cache-manager": "^6.4.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "flutterwave-node-v3": "^1.1.15",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.1.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.7",
    "redis": "^4.6.12",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "typescript": "^5.3.3"
  },
  "private": true
}
