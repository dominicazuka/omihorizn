# Copy this file to .env and fill in your values
# DO NOT commit .env to version control

# ============================================
# API CONFIGURATION
# ============================================
EXPO_PUBLIC_API_URL=http://localhost:5000/api
EXPO_PUBLIC_APP_NAME=OmiHorizn
EXPO_PUBLIC_ENVIRONMENT=development
EXPO_PUBLIC_API_KEY=...

# ============================================
# GOOGLE OAUTH
# ============================================
EXPO_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com

# ============================================
# AWS S3 CONFIGURATION
# ============================================
EXPO_PUBLIC_AWS_REGION=us-east-1

# ============================================
# FEATURE FLAGS
# ============================================
EXPO_PUBLIC_ENABLE_PREMIUM=true
EXPO_PUBLIC_ENABLE_ADMIN=true
EXPO_PUBLIC_ENABLE_INTERVIEW_PREP=true
EXPO_PUBLIC_ENABLE_SOP_GENERATOR=true

# ============================================
# SENTRY (Error Tracking - Optional)
# ============================================
EXPO_PUBLIC_SENTRY_DSN=https://your_sentry_dsn@sentry.io/project_id

# ============================================
# ANALYTICS (Optional)
# ============================================
EXPO_PUBLIC_ANALYTICS_ENABLED=true
EXPO_PUBLIC_MIXPANEL_TOKEN=your_mixpanel_token

# ============================================
# APP METADATA
# ============================================
EXPO_PUBLIC_APP_VERSION=1.0.0
EXPO_PUBLIC_BUILD_NUMBER=1
